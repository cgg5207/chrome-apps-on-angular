{
  "manifest_version": 2,
  "name": "weather chrome example",
  "description": "A plain text description",
  "version": "0.1",
  "permissions": ["http://api.wunderground.com/api/","http://api.map.baidu.com/telematics/v3/weather"],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}  